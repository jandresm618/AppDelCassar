{
	"QMainWindow":[
		{
			"tittle":"DC UI",
			"icon":":/images/icons/dc-logo.png",
			"frameless": true,
			"transluscentBg": true,
			"sizeGrip": "sizeGrip",
			
			"navigation":[
				{
					"minimize":"minimizeButton",
					"close": "closeWindowButton",
					"restore":[
						{	
							"buttonName": "restoreButton",
							"normalIcon": ":/icons/icons/original_svg/square.svg",
							"maximizedIcon": ":/icons/icons/original_svg/copy.svg"
						}
					],
					"moveWindow": "headerContainer",
					"tittleBar": "headerContainer"
				}
			]
		}
	],
	"QCustomSlideMenu": [{
		"name": "leftMenuContainer",
		"defaultSize": [{
			"width": 50,
			"height": "parent"
		}],
		"collapsedSize": [{
			"width": 50,
			"height": "parent"
		}],
		"expandedSize": [{
			"width": 150,
			"height": "parent"
		}],
		"toggleButton": [{
			"buttonName": "menuButton",
			"icons": [{
				"whenMenuIsCollapsed": ":/icons/icons/original_svg/align-justify.svg",
				"whenMenuIsExpanded": ":/icons/icons/original_svg/chevron-left.svg"
			}]
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 500,
			"animationEasingCurve": "Linear"
		}]
	}]
}
